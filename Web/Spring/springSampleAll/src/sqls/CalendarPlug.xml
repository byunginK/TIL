<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="CalendarPlug">
<select id="getCalendarPlugList" parameterType="bit.com.spring.dto.CalendarPlugDto" resultType="bit.com.spring.dto.CalendarPlugDto">
	SELECT SEQ, ID, SLEVEL, TITLE, CONTENT, STARTDATE, ENDDATE, WDATE
	FROM CALENDARPLUG
	WHERE ID = #{id}

</select>

</mapper>